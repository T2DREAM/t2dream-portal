        },
        "organism.scientific_name": {
            "title": "Organism"
        },
        "targets.investigated_as": {
            "title": "Target(s) of assay"
        },
         "targets.label": {
            "title": "Target(s) of assay"
        },
       "biosample_type": {
            "title": "Biosample type"
        },
       "biosample_term_name": {
            "title": "Biosample term"
        },
        "organ_slims": {
            "title": "Organ"
        },
        "relevant_life_stage": {
            "title": "Life stage"
        },
        "month_released": {
            "title": "Date released"
        },
        "software_used.software.name": {
            "title": "Software used"
        },
        "encyclopedia_version": {
            "title": "Encyclopedia version"
        },
        "lab.title": {
            "title": "Lab"
        },
        "award.project": {
            "title": "Project"
        },
        "award.rfa": {
            "title": "RFA"
        }
    },
    "columns": {
